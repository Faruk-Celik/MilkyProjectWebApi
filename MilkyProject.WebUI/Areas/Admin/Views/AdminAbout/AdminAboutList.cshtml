@model List<MilkyProject.WebUI.Dtos.AboutDto.ResultAboutDto>
@{
    ViewData ["Title"] = "AdminAboutList";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}
<table class="table">
    <thead>
        <tr>
            <th>#</th>
            <th>Başlık</th>
            <th>Açıklama</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.aboutId</td>
                <td>@item.title</td>
                <td>@item.description.</td>
              
            </tr>
        }
    </tbody>
</table>