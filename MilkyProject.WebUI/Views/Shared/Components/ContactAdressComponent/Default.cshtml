@model MilkyProject.WebUI.Dtos.AdressDto.ResultAdressDto
<div class="col-lg-6 wow fadeInUp" data-wow-delay="0.5s">
    <h3 class="mb-4">Contact Details</h3>
    <div class="d-flex border-bottom pb-3 mb-3">
        <div class="flex-shrink-0 btn-square bg-secondary rounded-circle">
            <i class="fa fa-map-marker-alt text-body"></i>
        </div>
        <div class="ms-3">
            <h6>Our Office</h6>
            <span>@Model.Street</span>
        </div>
    </div>
    <div class="d-flex border-bottom pb-3 mb-3">
        <div class="flex-shrink-0 btn-square bg-secondary rounded-circle">
            <i class="fa fa-phone-alt text-body"></i>
        </div>
        <div class="ms-3">
            <h6>Call Us</h6>
            <span>@Model.Phone</span>
        </div>
    </div>
    <div class="d-flex border-bottom-0 pb-3 mb-3">
        <div class="flex-shrink-0 btn-square bg-secondary rounded-circle">
            <i class="fa fa-envelope text-body"></i>
        </div>
        <div class="ms-3">
            <h6>Mail Us</h6>
            <span>@Model.Mail</span>
        </div>
    </div>

    <iframe class="w-100 rounded"
            src="<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d51130.20148767344!2d28.767722350416655!3d36.77926059396678!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14c07089a08968a5%3A0xae8122b3e307ec49!2zRGFsYW1hbiwgTXXEn2xhLCBUw7xya2l5ZQ!5e0!3m2!1sen!2sbd!4v1719852199072!5m2!1sen!2sbd" width="600" height="300" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>

</div>
